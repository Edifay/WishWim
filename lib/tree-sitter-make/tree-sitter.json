{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "make",
      "camelcase": "Make",
      "scope": "source.mk",
      "path": ".",
      "file-types": [
        "makefile",
        "Makefile",
        "MAKEFILE",
        "GNUmakefile",
        "mk",
        "mak",
        "dsp"
      ]
    }
  ],
  "metadata": {
    "version": "1.1.1",
    "license": "MIT",
    "description": "Makefile grammar for tree-sitter",
    "authors": [
      {
        "name": "Alexandre Muller"
      },
      {
        "name": "Amaan Qureshi",
        "email": "amaanq12@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter-grammars/tree-sitter-make"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
